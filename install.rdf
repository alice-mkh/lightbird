<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:id>$EXT_ID</em:id>
        <em:version>$VERSION</em:version>
        <em:type>2</em:type>
        <em:bootstrap>false</em:bootstrap>
        <em:unpack>true</em:unpack>
        <em:name>$EXT_NAME</em:name>
        <em:description>$EXT_DESC</em:description>
        <em:creator>Exalm</em:creator>
        <em:targetApplication>
            <Description>
                <em:id>$SM_ID</em:id>
                <em:minVersion>$SM_MIN_VERSION</em:minVersion>
                <em:maxVersion>$SM_MAX_VERSION</em:maxVersion>
            </Description>
        </em:targetApplication>
        <em:targetApplication>
            <Description>
                <em:id>$TB_ID</em:id>
                <em:minVersion>$TB_MIN_VERSION</em:minVersion>
                <em:maxVersion>$TB_MAX_VERSION</em:maxVersion>
            </Description>
        </em:targetApplication>
    </Description>
</RDF>